image,
text,
view {
  display: block;
  box-sizing: border-box;
}
button {
  border: none;
  border-radius: 50rpx;
}
button::after {
  border: none;
}
.text-show {
  display: -webkit-box;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
}
